{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "blubber-web",
  "version": "0.1.0",
  "identifier": "aurabirb.blubber-web",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420/babble-web/",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "blubber-web",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "pattern": {
        "use": "brownfield"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
